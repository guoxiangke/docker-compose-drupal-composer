{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/audiofield": "^1.6",
        "drupal/backup_migrate": "^4.0",
        "drupal/cdn": "^3.2",
        "drupal/ckeditor_markdown": "^1.0",
        "drupal/config_update": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.3",
        "drupal/devel": "^1.2",
        "drupal/eck": "^1.0@alpha",
        "drupal/examples": "1.x-dev",
        "drupal/features": "^3.7",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/fivestar": "1.x-dev",
        "drupal/flag": "^4.0@alpha",
        "drupal/ga_push": "^1.0@alpha",
        "drupal/google_analytics": "^2.2",
        "drupal/libraries": "^3.0@alpha",
        "drupal/markdown": "^1.2",
        "drupal/nodejs": "^1.1",
        "drupal/number_double": "^1.2",
        "drupal/oauth": "^2.1",
        "drupal/pathauto": "^1.2",
        "drupal/profile": "^1.0@RC",
        "drupal/rate": "^1.0@alpha",
        "drupal/restui": "^1.15",
        "drupal/s3fs": "^3.0@alpha",
        "drupal/services": "^4.0@beta",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplehtmldom": "2.x-dev",
        "drupal/token": "^1.3",
        "drupal/token_filter": "^1.0@beta",
        "drupal/viewfield": "^3.0@alpha",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/votingapi": "^3.0@alpha",
        "drupal/votingapi_widgets": "^1.0@alpha",
        "drupal/webprofiler": "^2.0@RC",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        }
    }
}
